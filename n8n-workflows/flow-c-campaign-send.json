    {
      "parameters": {
        "operation": "executeQuery",
        "query": "=INSERT INTO error_log (workflow_name, error_message, payload, ts)\nVALUES (\n  'campaign-send',\n  '{{ $json.error.message }}',\n  '{{ JSON.stringify($json) }}',\n  NOW()\n);",
        "options": {}
      },
      "name": "Log Error to Database",
