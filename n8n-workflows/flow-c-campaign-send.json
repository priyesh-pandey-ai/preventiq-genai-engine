        "httpHeaderAuth": {
          "id": "resend-api",
          "name": "Resend API"
        }
      }
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "=INSERT INTO assignments (lead_id, persona_id, variant_subject_id, corr_id, status, send_at)\nVALUES (\n  '{{ $('Split campaigns array').item.json.lead_id }}',\n  '{{ $('Split campaigns array').item.json.persona_id }}',\n  '{{ $('Split campaigns array').item.json.variant_id }}',\n  '{{ $json.id }}',\n  'sent',\n  NOW()\n)\nRETURNING id;",
        "options": {}
      },
      "name": "Store Assignment in Supabase",
      "type": "n8n-nodes-base.postgres",
      "typeVersion": 2.4,
      "position": [
        1340,
        200
